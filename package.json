{
	"name": "@gandolfini/hooks",
	"version": "0.0.3",
	"description": "Esoteric Hooks",
	"type": "module",
	"main": "./dist/gandolfini_hooks.umd.js",
	"module": "./dist/gandolfini_hooks.es.js",
	"files": [
		"./dist/**/*"
	],
	"exports": {
		".": {
			"import": "./dist/gandolfini_hooks.es.js",
			"require": "./dist/gandolfini_hooks.umd.js"
		}
	},
	"scripts": {
		"build": "rollup -c",
		"clean": "rimraf dist",
		"start": "yarn storybook",
		"lint": "eslint \"{src, tests}/**/*.ts\"",
		"lint:fix": "yarn lint --fix",
		"storybook": "start-storybook -p 6006",
		"storybook:build": "build-storybook"
	},
	"dependencies": {
		"rollup": "^2.79.0",
		"rollup-plugin-sass": "^1.2.13",
		"rollup-plugin-typescript2": "^0.33.0",
		"rollup-plugin-uglify": "^6.0.4"
	},
	"devDependencies": {
		"@babel/core": "^7.19.0",
		"@open-wc/building-rollup": "^2.0.2",
		"@storybook/addon-actions": "^6.5.10",
		"@storybook/addon-essentials": "^6.5.10",
		"@storybook/addon-interactions": "^6.5.10",
		"@storybook/addon-links": "^6.5.10",
		"@storybook/builder-vite": "^0.2.2",
		"@storybook/react": "^6.5.10",
		"@storybook/testing-library": "^0.0.13",
		"@types/react": "^18.0.17",
		"@types/react-dom": "^18.0.6",
		"@typescript-eslint/eslint-plugin": "^5.36.2",
		"@typescript-eslint/parser": "^5.36.2",
		"@vitejs/plugin-react": "^2.1.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.5",
		"babel-plugin-dynamic-import-node": "^2.3.3",
		"deepmerge": "^4.2.2",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-react-app": "^7.0.1",
		"eslint-plugin-react": "^7.31.7",
		"prettier": "^2.7.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rimraf": "^3.0.2",
		"typescript": "^4.6.4"
	},
	"repository": "https://github.com/FooOperator/my-hooks.git",
	"author": "FooOperator <foxoperator1@gmail.com>",
